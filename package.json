{
  "name": "nodetools",
  "bin": "main.js",
  "version": "1.0.0",
  "description": "Multitool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.electron.nodetools",
    "productName": "NodeTools",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "deb",
      "icon": "build/icon.png",
      "maintainer": "iTzPhil",
      "executableName": "NodeTools",
      "desktop": {
        "Name": "NodeTools",
        "Comment": "Multi purpose tool build with Node and Electron",
        "Terminal": "false",
        "Type": "Application",
        "Icon": "build/icon.png"
      }
    },
    "files": [
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "generate-password": "^1.7.0",
    "qrcode": "^1.5.1",
    "steggy": "^2.0.1",
    "uuid": "^9.0.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "electron": "^25.3.2"
  }
}